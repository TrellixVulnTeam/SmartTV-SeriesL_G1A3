defines = -DV8_DEPRECATION_WARNINGS -DCLD_VERSION=2 -D_FILE_OFFSET_BITS=64 $
    -DDISABLE_NACL -DCHROMIUM_BUILD -DCR_CLANG_REVISION=264915-1 $
    -DUI_COMPOSITOR_IMAGE_TRANSPORT -DUSE_AURA=1 -DUSE_OZONE=1 $
    -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DENABLE_MEDIA_ROUTER=1 -DUSE_PROPRIETARY_CODECS -DENABLE_BROWSER_CDMS $
    -DENABLE_TOPCHROME_MD=1 -DFIELDTRIAL_TESTING_ENABLED $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_AUTOFILL_DIALOG=1 $
    -DENABLE_PRINTING=1 -DENABLE_BASIC_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_APP_LIST=1 $
    -DENABLE_SETTINGS_APP=1 -DENABLE_SUPERVISED_USERS=1 -DENABLE_MDNS=1 $
    -DENABLE_SERVICE_DISCOVERY=1 -DVIDEO_HOLE=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA $
    -DENABLE_YOUTUBE_ISTYPESUPPORTED_EXTENSIONS=1 $
    -DENABLE_EXTERNAL_ISTYPESUPPORTED==1 -DUSE_OPENSSL=1 -DUSE_NSS_CERTS=1 $
    -DUSE_NSS_VERIFIER=1 -DNFBE_DTV_SOCIO -DNFBE_DTV_SOCIO_DL11 -DNDEBUG $
    -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0
includes = -Igen -I../../third_party/boringssl/src/include
cflags = -fstack-protector --param=ssp-buffer-size=4 -pthread $
    -fno-strict-aliasing -Wno-extra -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-unused-local-typedefs -march=armv8-a -mtune=cortex-a53 $
    -mfpu=crypto-neon-fp-armv8 -mfloat-abi=hard -mthumb $
    --sysroot=/work/bsp/sc1400 -O2 -fno-ident -fdata-sections $
    -ffunction-sections -funwind-tables
cflags_c = -I/work/bsp/sc1400/usr/local/include $
    -I/work/bsp/sc1400/usr/include -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr $
    -I/work/bsp/sc1400/usr/local/include -I/work/bsp/sc1400/usr/include $
    -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr
cflags_cc = -I/work/bsp/sc1400/usr/local/include $
    -I/work/bsp/sc1400/usr/include -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr $
    -fno-exceptions -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wno-deprecated -Wno-abi $
    -Wno-literal-suffix -std=gnu++11 -Wno-narrowing
arflags = 

build $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.aes-armv4.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/aes/aes-armv4.S
build $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.aesv8-armx32.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/aes/aesv8-armx32.S
build $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.bsaes-armv7.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/aes/bsaes-armv7.S
build $
    obj/third_party/boringssl/linux-arm/crypto/bn/boringssl_asm.armv4-mont.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/bn/armv4-mont.S
build $
    obj/third_party/boringssl/linux-arm/crypto/chacha/boringssl_asm.chacha-armv4.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/chacha/chacha-armv4.S
build $
    obj/third_party/boringssl/linux-arm/crypto/modes/boringssl_asm.ghash-armv4.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/modes/ghash-armv4.S
build $
    obj/third_party/boringssl/linux-arm/crypto/modes/boringssl_asm.ghashv8-armx32.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/modes/ghashv8-armx32.S
build $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha1-armv4-large.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/sha/sha1-armv4-large.S
build $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha256-armv4.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/sha/sha256-armv4.S
build $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha512-armv4.o: $
    cc ../../third_party/boringssl/linux-arm/crypto/sha/sha512-armv4.S
build $
    obj/third_party/boringssl/src/crypto/curve25519/asm/boringssl_asm.x25519-asm-arm.o: $
    cc ../../third_party/boringssl/src/crypto/curve25519/asm/x25519-asm-arm.S
build $
    obj/third_party/boringssl/src/crypto/poly1305/boringssl_asm.poly1305_arm_asm.o: $
    cc ../../third_party/boringssl/src/crypto/poly1305/poly1305_arm_asm.S

build obj/third_party/boringssl/libboringssl_asm.a: alink_thin $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.aes-armv4.o $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.aesv8-armx32.o $
    obj/third_party/boringssl/linux-arm/crypto/aes/boringssl_asm.bsaes-armv7.o $
    obj/third_party/boringssl/linux-arm/crypto/bn/boringssl_asm.armv4-mont.o $
    obj/third_party/boringssl/linux-arm/crypto/chacha/boringssl_asm.chacha-armv4.o $
    obj/third_party/boringssl/linux-arm/crypto/modes/boringssl_asm.ghash-armv4.o $
    obj/third_party/boringssl/linux-arm/crypto/modes/boringssl_asm.ghashv8-armx32.o $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha1-armv4-large.o $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha256-armv4.o $
    obj/third_party/boringssl/linux-arm/crypto/sha/boringssl_asm.sha512-armv4.o $
    obj/third_party/boringssl/src/crypto/curve25519/asm/boringssl_asm.x25519-asm-arm.o $
    obj/third_party/boringssl/src/crypto/poly1305/boringssl_asm.poly1305_arm_asm.o
