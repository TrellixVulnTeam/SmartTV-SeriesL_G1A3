build obj/third_party/mesa/osmesa.actions_depends.stamp: stamp $
    obj/third_party/mesa/libmesa.a $
    obj/third_party/mesa/libmesa_libglslcommon.a

defines = '-DMAPI_ABI_HEADER="glapi_mapi_tmp_shared.h"' $
    '-DPACKAGE_NAME="Mesa"' '-DPACKAGE_TARNAME="mesa"' $
    '-DPACKAGE_VERSION="9.0.3"' '-DPACKAGE_STRING="Mesa\ 9.0.3"' $
    '-DPACKAGE_BUGREPORT="https://bugs.freedesktop.org/enter_bug.cgi\?product=Mesa"' $
    '-DPACKAGE_URL=""' '-DPACKAGE="mesa"' '-DVERSION="9.0.3"' $
    -DSTDC_HEADERS=1 -DHAVE_SYS_TYPES_H=1 -DHAVE_SYS_STAT_H=1 $
    -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_MEMORY_H=1 $
    -DHAVE_STRINGS_H=1 -DHAVE_INTTYPES_H=1 -DHAVE_STDINT_H=1 $
    -DHAVE_DLFCN_H=1 '-DLT_OBJDIR=".libs/"' -DYYTEXT_POINTER=1 $
    -DHAVE_LIBEXPAT=1 -DHAVE_LIBXCB_DRI2=1 -DFEATURE_GL=1 -DMAPI_MODE_GLAPI $
    -DIN_DRI_DRIVER -DUSE_XCB -DGLX_INDIRECT_RENDERING $
    -DGLX_DIRECT_RENDERING -DUSE_EXTERNAL_DXTN_LIB=1 -DIN_DRI_DRIVER $
    -DHAVE_ALIAS -DHAVE_MINCORE -DHAVE_LIBUDEV -D_GLAPI_NO_EXPORTS $
    -DV8_DEPRECATION_WARNINGS -DCLD_VERSION=2 -D_FILE_OFFSET_BITS=64 $
    -DDISABLE_NACL -D_GNU_SOURCE -DHAVE_DLOPEN -DHAVE_PTHREAD=1 $
    -DHAVE_UNISTD_H=1 -DHAVE_POSIX_MEMALIGN -DCHROMIUM_BUILD $
    -DCR_CLANG_REVISION=264915-1 -DUI_COMPOSITOR_IMAGE_TRANSPORT $
    -DUSE_AURA=1 -DUSE_OZONE=1 -DUSE_DEFAULT_RENDER_THEME=1 $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_MEDIA_ROUTER=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_BROWSER_CDMS -DENABLE_TOPCHROME_MD=1 $
    -DFIELDTRIAL_TESTING_ENABLED -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGINS=1 -DENABLE_SESSION_SERVICE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DENABLE_PRINTING=1 $
    -DENABLE_BASIC_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_APP_LIST=1 $
    -DENABLE_SETTINGS_APP=1 -DENABLE_SUPERVISED_USERS=1 -DENABLE_MDNS=1 $
    -DENABLE_SERVICE_DISCOVERY=1 -DVIDEO_HOLE=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA $
    -DENABLE_YOUTUBE_ISTYPESUPPORTED_EXTENSIONS=1 $
    -DENABLE_EXTERNAL_ISTYPESUPPORTED==1 -DMESA_EGL_NO_X11_HEADERS $
    -DUSE_OPENSSL=1 -DUSE_NSS_CERTS=1 -DUSE_NSS_VERIFIER=1 -DNFBE_DTV_SOCIO $
    -DNFBE_DTV_SOCIO_DL11 -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0
includes = -Igen -I../../third_party/mesa/src/src/mapi $
    -I../../third_party/mesa/src/src/mesa $
    -I../../third_party/mesa/src/src/mesa/drivers $
    -I../../third_party/mesa/src/chromium_gensrc/mesa $
    -I../../third_party/mesa/src/include
cflags = -fstack-protector --param=ssp-buffer-size=4 -pthread $
    -fno-strict-aliasing -Wno-extra -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-unused-local-typedefs -fPIC -march=armv8-a -mtune=cortex-a53 $
    -mfpu=crypto-neon-fp-armv8 -mfloat-abi=hard -mthumb $
    --sysroot=/work/bsp/sc1400 -O2 -fno-ident -fdata-sections $
    -ffunction-sections -funwind-tables
cflags_c = -I/work/bsp/sc1400/usr/local/include $
    -I/work/bsp/sc1400/usr/include -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr $
    -I/work/bsp/sc1400/usr/local/include -I/work/bsp/sc1400/usr/include $
    -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr
cflags_cc = -I/work/bsp/sc1400/usr/local/include $
    -I/work/bsp/sc1400/usr/include -I/work/bsp/sc1400/usr/include/freetype2 $
    -I/work/bsp/sc1400/usr/include/dbus-1.0 $
    -I/work/bsp/sc1400/usr/include/nss -I/work/bsp/sc1400/usr/include/nspr $
    -fno-exceptions -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wno-deprecated -Wno-abi $
    -Wno-literal-suffix -std=gnu++11 -Wno-narrowing
arflags = 

build $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o: $
    cc ../../third_party/mesa/src/src/mesa/drivers/common/driverfuncs.c
build obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/common/meta.c
build obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.c

ldflags = -L/work/bsp/sc1400/usr/local/lib -L/work/bsp/sc1400/lib $
    -L/work/bsp/sc1400/usr/lib -Wl,-z,now -Wl,-z,relro -Wl,-z,defs -pthread $
    -Wl,-z,noexecstack -fPIC -Wl,--disable-new-dtags $
    --sysroot=/work/bsp/sc1400 -L/work/bsp/sc1400/lib/ $
    -Wl,-rpath-link=/work/bsp/sc1400/lib/ -L/work/bsp/sc1400/usr/lib/ $
    -Wl,-rpath-link=/work/bsp/sc1400/usr/lib/ $
    -L/work/bsp/sc1400/usr/dtvrec/lib/ $
    -Wl,-rpath-link=/work/bsp/sc1400/usr/dtvrec/lib/ $
    -L/work/bsp/sc1400/usr/local/lib/ $
    -Wl,-rpath-link=/work/bsp/sc1400/usr/local/lib/ $
    -L/work/bsp/sc1400/usr/local/netfrontbe/lib/ $
    -Wl,-rpath-link=/work/bsp/sc1400/usr/local/netfrontbe/lib/ -Wl,-O1 $
    -Wl,--as-needed -Wl,--gc-sections
libs =  -ldl -lm -lstdc++
build libosmesa.so libosmesa.so.TOC: solink_module $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o $
    obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o $
    obj/third_party/mesa/libmesa.a $
    obj/third_party/mesa/libmesa_libglslcommon.a || $
    obj/third_party/mesa/osmesa.actions_depends.stamp
  soname = libosmesa.so
  lib = libosmesa.so
  link_file_list = libosmesa.so.rsp
