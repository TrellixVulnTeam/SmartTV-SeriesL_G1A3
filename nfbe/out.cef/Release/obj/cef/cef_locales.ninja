      repack_locales_a4555f22cf1fa97c7b2ea1dc9b1c5d10
  restat = 1

build cef_repack_locales.stamp: $
    cef_locales_target_repack_locales_a4555f22cf1fa97c7b2ea1dc9b1c5d10 $
    ../../cef/tools/repack_locales.py || $
